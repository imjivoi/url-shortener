{
  "tabWidth": 2,
  "printWidth": 120,
  "singleQuote": true,
  "semi": false,
  "overrides": [
    {
      "files": ["*.json", "*.vue"],
      "options": {
        "htmlWhitespaceSensitivity": "ignore"
      }
    }
  ],
  "arrowParens": "always",
  "trailingComma": "all",
  "endOfLine": "auto"
}
